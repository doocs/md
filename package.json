{
  "name": "md",
  "type": "module",
  "version": "1.6.0",
  "private": false,
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build:only {@}\" --",
    "build:only": "vite build",
    "build:h5-netlify": "vite build --mode netlify",
    "build:cli": "npm run build && npm run shx rm -rf md-cli/dist && npm run shx rm -rf dist/**/*.map && npm run shx cp -r dist md-cli/ && cd md-cli && npm run pack",
    "preview": "npm run build && vite preview",
    "release:cli": "node ./bin/release.js",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --build --force",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "ali-oss": "^6.17.1",
    "axios": "^1.6.0",
    "buffer-from": "^1.1.2",
    "codemirror": "^5.65.7",
    "core-js": "^3.34.0",
    "cos-js-sdk-v5": "^1.3.9",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.7.6",
    "form-data": "4.0.0",
    "highlight.js": "^11.6.0",
    "juice": "^8.0.0",
    "katex": "^0.16.10",
    "marked": "^4.0.18",
    "marked-katex-extension": "^4.0.3",
    "minio": "7.0.33",
    "node-fetch": "^3.2.10",
    "pinia": "^2.1.6",
    "qiniu-js": "^3.4.1",
    "uuid": "^8.3.2",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.3",
    "@unocss/eslint-plugin": "^0.61.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "async-validator": "^4.0.7",
    "babel-plugin-import": "^1.13.3",
    "cache-loader": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.2",
    "less": "^4.1.2",
    "less-loader": "^7.3.0",
    "mini-types": "*",
    "miniprogram-api-typings": "*",
    "npm-run-all": "^4.1.5",
    "postcss-comment": "^2.0.0",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "shx": "^0.3.4",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.2.2",
    "unocss": "^0.61.2",
    "vite": "^5.3.1",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vue-tsc": "^2.0.21"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "npm run lint"
  }
}
